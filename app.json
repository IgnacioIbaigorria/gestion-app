{
  "expo": {
    "name": "Gesti√≥n App",
    "slug": "punto-eco-gestion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/puntoeco.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/puntoeco_splash_1280.png",
      "resizeMode": "cover",
      "backgroundColor": "#91b578"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResources": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/puntoeco.png",
        "backgroundColor": "#91b578",
        "resizeMode": "contain"
      },
      "package": "com.ignacioivan00.puntoecogestion"
    },
    "assetBundlePatterns": [
      "assets/images/*"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResources": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4ecc3add-1669-449d-aef2-b6a185bd24cd"
      }
    }
  }
}